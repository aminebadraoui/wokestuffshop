    # Tools
    pod 'SwiftGen',                           '~> 4.2'

  target 'wokestuffshopTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking',               '~> 4.0'
    pod 'RxTest',                   '~> 4.0'
    pod 'Mockingjay',               '~> 2.0.1'
  end

  target 'wokestuffshopUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
